#include <iostream>
#include <iomanip>
using namespace std;
int main()
{
  int month, choice ;
 
  cout << "Enter a month(number) " << endl;
  cin >> choice;
  
  switch (choice)
    {
    
    case 1: 
    case 2: 
    case 12:
      cout << "Winter" ;
      break;
     
    case 3:
    case 4:
    case 5:
      cout << "Spring" ;
      break ;
     
     
    case 6: 
    case 7:
    case 8:
      cout << "Summer" ;
      break ;
    
     
    case 9: 
    case 10:
    case 11:
      cout << "Fall" ;
      break ;

    default:
      cout << "Invalid month" << endl ;  
    
    }
  return 0;
}
