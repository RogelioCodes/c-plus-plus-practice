#include<iostream>
#include<iomanip>

using namespace std; 
const int size = 5 ;
int main() 
{
  bool yas = false ; 
  int tail ; 
  int head ; 
  int ar[size] = {10,20,30,40,50} ;
  int sum = 30 ; 
  for(int i = 0 ; i < size ; i++)
    {
      
      ar[size-1] = tail ; 
      ar[i] = head ;
      if(tail + head == sum)
	yas = true ; 
      
    }
  cout << "Yas is: " << yas ; 
  return 0 ; 
}
